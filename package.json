{
  "name": "react-editor",
  "version": "1.0.0",
  "description": "A react component reply on quill.js",
  "main": "umd/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "rimraf umd && npm run build-umd && npm run build-min",
    "build-umd": "cross-env NODE_ENV=development webpack src/index.tsx -o umd/react-editor.js",
    "build-min": "cross-env NODE_ENV=production webpack src/index.tsx -o umd/react-editor.min.js",
    "demo": "webpack-dev-server --config demo/webpack.config.js --open",
    "build:demo": "webpack --config demo/webpack.config.js --mode development",
    "lint": "eslint './src/**/*.?(ts|tsx)'",
    "autofix": "eslint './src/**/*.?(ts|tsx)' --fix",
    "analyse": "cross-env NODE_ENV=analyse webpack src/index.ts -o umd/react-editor.js",
    "tsc": "tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mike-han/react-editor.git"
  },
  "author": "mike-han",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mike-han/react-editor/issues"
  },
  "homepage": "https://github.com/mike-han/react-editor#readme",
  "files": [
    "umd/",
    "types/",
    "types.d.ts",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-react": "^7.12.7",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@types/lodash-es": "^4.17.3",
    "@types/quill": "^2.0.9",
    "@types/react": "^16.9.56",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-loader": "^8.2.1",
    "copy-webpack-plugin": "^6.3.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.5.7",
    "html-webpack-plugin": "^5.0.0-alpha.9",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.10",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.15",
    "quill": "1.3.7",
    "react": "^17.0.1"
  }
}
